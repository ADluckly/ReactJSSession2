{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "defaultSeverity": "warning",
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "max-classes-per-file": false,
    "no-console": false,
    "ordered-imports": false,
    "jsx-self-close": false,
    "jsx-boolean-value": false
  }
}
